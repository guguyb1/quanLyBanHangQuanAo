
<?xml version="1.0" encoding="UTF-8"?>

<?import javafx.scene.control.Alert?>
<?import javafx.scene.control.ButtonType?>
<?import javafx.scene.control.Button?>
<?import javafx.scene.layout.VBox?>

<?import javafx.scene.control.ButtonBar?>
<Alert fx:id="alert" xmlns="http://javafx.com/javafx/16" xmlns:fx="http://javafx.com/fxml/1" type="CONFIRMATION">
    <contentText>Xác nhận xóa tệp?</contentText>
    <headerText>Xác nhận</headerText>
    <Button>
        <ButtonBar>
            <buttons>
                <ButtonType fx:id="buttonOK" text="OK" />
                <ButtonType fx:id="buttonCancel" text="Cancel" />
            </buttons>
        </ButtonBar>
    </Button>
</Alert>
